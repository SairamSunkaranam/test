 pipeline {

  agent any
  parameters {
      choice(name: "env1", choices: "qa\nprod")
  }
  
  stages {
    stage ('test'){
       steps {
       
         script {
             echo "Hello world"
             echo "$env1/${env1}"
         
             if ("$env1" == "qa" ) {
               QA_BUCKET = "test-value1"
               C3_PREFIX = "my-provate-value"
             }      
         
             echo "${QA_BUCKET}/${env1}.tfstater"
             echo "${C3_PREFIX}/${env1}.tfstater"
             
             wget https://releases.hashicorp.com/terraform/0.13.3/terraform_0.13.3_linux_amd64.zip
             wget https://raw.githubusercontent.com/SairamSunkaranam/test/master/test/main.tf
             ./terraform init
             
         }
       }
    }
  }

}
